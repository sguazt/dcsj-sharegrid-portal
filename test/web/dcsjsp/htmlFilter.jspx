<?xml version="1.0" encoding="UTF-8"?>

<jsp:root version="2.1"
	xmlns:jsp="http://java.sun.com/JSP/Page"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:dcsjsp="http://dcs.di.unipmn.it/tld/dcsjsp">

	<jsp:directive.page contentType="text/html;charset=UTF-8" pageEncoding="UTF-8"/>
        <jsp:output	omit-xml-declaration="no"
			doctype-root-element="html"
			doctype-public="-//W3C//DTD XHTML 1.0 Transitional//EN"
			doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-transitional.dtd"/>

	<f:view>
	<html xmlns="http://www.w3.org/1999/xhtml">
		<head>
			<title>DCS *htmlFilter* component test</title>
		</head>
		<body>
			<h1>DCS *htmlFilter* component test</h1>

			<h:panelGrid columns="2">
				<h:outputText value="Array of Strings"/>
				<h:panelGroup>
					<dcsjsp:htmlFilter>
					<![CDATA[<html><body>&lt;</body></html>]]>
					<br/>
					<h:outputText value="#{msgs.helpJobSubmitJobFileMsg}"/>
					</dcsjsp:htmlFilter>
				</h:panelGroup>
			</h:panelGrid>
		</body>
	</html>
	</f:view>
</jsp:root>
